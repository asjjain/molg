import { CommonModule } from '@angular/common';
import { NgModule } from '@angular/core';
import { RouterModule, Routes } from '@angular/router';
const appRoute: Routes = [
 
]



@NgModule({
  imports: [RouterModule.forRoot(appRoute,{useHash :true}),
    CommonModule
  ],
  declarations: [],
  exports: [RouterModule]
})
export class RoutingModuleModule { }

********************************************************************************
row-operation.component.html
********************************************************************************
<div>
<!-- <span class="glyphicon glyphicon-ok"></span>
<span class="glyphicon glyphicon-remove"></span> -->
<button type="button" class="btn btn-sm btn-success"*ngIf="isValid"
 style="width:79px;padding:1px 11px;margin-bottom:6px;" (click)="validateCMAccountRegion()">
 Validate</button>
<button type="button" class="btn btn-sm btn-danger" *ngIf="isDeactivate" 
style="padding:1px 11px;margin-bottom:6px;" (click)="deactivateCMAccountRegion()"
>
Deactivate</button>


</div>

********************************************************************************
row-operation.component.ts
********************************************************************************

import { Component, ElementRef, ViewChild } from '@angular/core';
import { INoRowsOverlayAngularComp } from 'ag-grid-angular';
import { AccountTableService } from '../shared/account-table.service';

@Component({
  selector: 'app-row-operation',
  templateUrl: './row-operation.component.html',
  styleUrls: ['./row-operation.component.css']
})

export class RowOperationComponent implements  INoRowsOverlayAngularComp  {
 
 
  @ViewChild('comments') searchElement: ElementRef;
  
  private userName: String = sessionStorage.getItem("userDetails");
  isValid:boolean =false;
  isDeactivate:boolean =false;
  private selectedRowObject:any;
  private accounts: any;
  private parameters:any;
  

  constructor(private accountTableService: AccountTableService,
    private el: ElementRef) { 
  }
    agInit(params){
      this.parameters =params.data;
        if(params.data.createdBy != this.userName && (params.data.status=="CREATED" || params.data.status=="DEACTIVATED"))
        {
          this.isValid=true;
        }
        if(params.data.createdBy != this.userName && params.data.status=="VALIDATED")
        {
          this.isDeactivate=true;
        }
    }
  

validateCMAccountRegion() {
  console.log("Validate Account method called");
  

  this.selectedRowObject ={
    accountNumber:this.parameters.accountNumber,
    region:this.parameters.region,
    status:this.parameters.status,
    createdBy :this.parameters.createdBy,
    creationDate:this.parameters.creationDate,
    validatedBy:sessionStorage.getItem("userDetails"),
    comments:this.parameters.comments,
    type:'validate'
  }
  this.accountTableService.loadModal.next(this.selectedRowObject);
  console.log("validate Object:", this.selectedRowObject);
  
}

deactivateCMAccountRegion()
{
  console.log("Deactivate Account method called");
  this.selectedRowObject ={
    accountNumber:this.parameters.accountNumber,
    region:this.parameters.region,
    status:this.parameters.status,
    createdBy :this.parameters.createdBy,
    creationDate:this.parameters.creationDate,
    validatedBy:sessionStorage.getItem("userDetails"),
    comments:this.parameters.comments,
    type:'deactivate'
  }
  this.accountTableService.loadModal.next(this.selectedRowObject);
  console.log("deactivate Object:", this.selectedRowObject);
  
}
 
}







