corporate-action-nack-reason.component.ts
********************************************************************************


import { Component, ElementRef } from '@angular/core';
import { INoRowsOverlayAngularComp } from 'ag-grid-angular';
import { AccountTableService } from '../shared/account-table.service';

@Component({
  selector: 'app-corporate-action-nack-reason',
  templateUrl: './corporate-action-nack-reason.component.html',
  styleUrls: ['./corporate-action-nack-reason.component.css']
})
export class CorporateActionNackReasonComponent implements INoRowsOverlayAngularComp {
  parameters:any;
  private selectedSwiftMessage :any;
  activateButton:boolean;
  //@ViewChild('swiftMessage') searchElement: ElementRef;

  constructor(private accountTableService: AccountTableService,
    private el: ElementRef) { }

    agInit(params){
      this.activateButton=false;
      this.parameters =params.data['nackReason'];
      if(params.data['status'] === "NAK"){
        this.activateButton=true;
      }
     }
   
     ViewNackReason()
     {
       console.log("View Nack Reason called");
       let data={
         data:this.parameters,
         heading:'Nack Reason'
       }
       this.accountTableService.loadSwiftModal.next(data);
     }

}

