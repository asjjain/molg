********************************************************************************
recipient-data.component.ts
********************************************************************************

import { Component, OnDestroy, OnInit, ViewChild } from '@angular/core';
import { ModalManager } from 'ngb-modal';
import { Subscription } from 'rxjs/internal/Subscription';
import { ActiveRecipientComponent } from 'src/app/active-recipient/active-recipient.component';
import { RecipientDetails } from 'src/assets/recipientDetails';
import { AccountTableService } from '../shared/account-table.service';

@Component({
  selector: 'app-recipient-data',
  templateUrl: './recipient-data.component.html',
  styleUrls: ['./recipient-data.component.css']
})
export class RecipientDataComponent implements OnInit,OnDestroy {
 
  reloadEmailFaxGrid:Subscription;
  channel:String;
  isEmail:boolean;
  isFax:boolean;
  recipientId:String;
  private recipientIdOld :String;
  private email:string;
  private faxNumber:String;
  private secondfaxNumber:String;
  private activated:boolean;
  private recipientDetail :RecipientDetails;
  recipientDetails;
  private recipientDetailsObj;
  private gridApi;
  private gridColumnApi;
  recipientDetailsColumnDefs;
  searchText:String;
  icons;
  private recipientIdNotValid:boolean;
  private channelNotValid :boolean;
  private faxNotValid:boolean;
  rowSelection;
  private data;
  comments;
  updatedComments;
  private emailAddress :string[];
  isCommentsValid:boolean =false;
  private selectedRecipientId;
  private selectedFaxNumber;
  private selectedEmailAddress;
  private selectedSecondFaxNumber;
  private  selectedChannel;
  private  selectedComments;
  private ind :number =0;



  public emailForm: {
    emails: Email[];
    };


  constructor(private accountService: AccountTableService,private modalService: ModalManager) { 

    this.
    recipientDetailsColumnDefs = [
      {headerName: 'Operations',cellRendererFramework:ActiveRecipientComponent,width:100},
      {headerName: 'Recipient Id', field: 'recipientId',width:100},
      {headerName: 'Channel', field: 'communicationMode',width:100},
      {headerName: 'Fax Number', field: 'faxNumber',width:100},
      {headerName: 'Second Fax Number', field: 'secondFaxNumber',width:100},
      {headerName: 'Email Address', field: 'emailAddress',width:130},
      {headerName: 'Status', field: 'status',width:70},
      {headerName: 'Created By', field: 'createdBy' ,width:100},
      {headerName: 'Creation Date', field: 'creationDate' ,width:100},
      {headerName: 'Updated By', field: 'updatedBy',width:100},
      {headerName: 'Updated Date', field: 'updatedDate',width:100},
      {headerName: 'Comments', field: 'comments',width:100}
    ];

    this.icons = {
      filter: ' '
    };

    this.rowSelection ='single';

    this.emailForm = {
      emails: []
      };
       
     
  }

  @ViewChild('myEmailFaxModal') myEmailFaxModal;
  private emailFaxModalRef;

  ngOnInit() {
    console.log("ngOnInit Called");
    this.accountService.getMonitoringDetails
    ("/MOLandingZoneGateway/api/emailFax/details")
      .subscribe(recipientDetails => 
       { this.recipientDetails = recipientDetails;
        console.log(this.recipientDetails)
        if(this.recipientDetails ==null)
        {
          this.accountService.showError('Unable to retrieve the account details');
        }
       });

       this.reloadEmailFaxGrid=this.accountService.reloadEmailFaxGrid$.subscribe(data=>{
        this.recipientDetails=data;
      });
    
    this.accountService.loadEmailFaxModal.subscribe(data=>{
      this.data=data;
      this.openModal();
    })
    
  }

  // I add a new email record to the form-model.
public addEmails() : void {  
//debugger

 this.emailForm.emails.push({
 id: this.ind, // <--- uniqueness hook.
 email :""
 });
  
 }

 onClick(event){
   let emailIndex:number = event.target.name;
   this.ind = event.target.id;
   console.log(this.emailForm.emails.length);
   
   this.removeEmails(emailIndex);
 }


public removeEmails(index:number) : void {
 this.emailForm.emails.splice( index, 1 );
 console.log(this.emailForm.emails);
 }

  cancelEdit()
  {
    this.recipientId ="";
    this.email ="";
    this.faxNumber="";
    this.secondfaxNumber ="";
    this.channel="";
    this.comments="";
    this.emailForm.emails =[];
    this.ind=0;
  }

  openModal(){
    
      this.emailFaxModalRef = this.modalService.open(this.myEmailFaxModal, {
          size: "md",
          modalClass: 'myEmailFaxModal',
          hideCloseButton: false,
          centered: false,
          backdrop: false,
          animation: false,
          keyboard: false,
          closeOnOutsideClick: false,
     
         
      })
    }

    closeModal(commentsValid){
      
      
      console.log(commentsValid)
      
      if(commentsValid.control.status =="INVALID")
      {
          this.isCommentsValid =true;
      }
      else{
  
        this.isCommentsValid =false;
      
        this.modalService.close(this.emailFaxModalRef);
        this.data.comments=this.updatedComments;
       
          if(this.data.type=='activate')
          {
            this.activateRecipient();
          }
    
          if(this.data.type=='deactivate')
          {
            this.deactivateRecipient();
          }
        }
       
    }

    activateRecipient()
    {
      console.log("comments:",this.data.commments)
      this.accountService.validateCMAccountRegion
      ("/MOLandingZoneGateway/api/emailFax/activate",this.data)
        .subscribe(data => {
          console.log(data);
          if (data == 1) {
            console.log("Activated Successfully");
            this.accountService.showSuccess('Activated Successfully');
            this.accountService.getMonitoringDetails
            ("/MOLandingZoneGateway/api/emailFax/details")
            .subscribe(recipientDetails => 
              { //this.accounts = accounts;
               if(recipientDetails ==null)
               {
                 this.accountService.showError('Unable to retrieve the recipient details');
               }
               else{
                this.accountService.reloadEmailFaxGrid$.next(recipientDetails);
               }
              });
            //.subscribe(accounts => this.accountTableService.reloadGrid$.next(accounts));
           // console.log(this.validatedObj);
          }
          else if(data == -1)
          {
            console.log("UnExpected error");
            this.accountService.showError('Unexpected Error Occurred');
          }
        });
    }

    deactivateRecipient()
    {
      this.accountService.validateCMAccountRegion
      ("/MOLandingZoneGateway/api/emailFax/deactivate", this.data)
      .subscribe(data => {
        console.log(data);
        if (data == 1) {
          console.log("Deactivated Successfully");
          this.accountService.showSuccess('Deactivated Successfully');
          this.accountService.getMonitoringDetails
          ("/MOLandingZoneGateway/api/emailFax/details")
          .subscribe(recipientDetails => 
            { //this.accounts = accounts;
             if(recipientDetails ==null)
             {
               this.accountService.showError('Unable to retrieve the account details');
             }
             else{
              this.accountService.reloadEmailFaxGrid$.next(recipientDetails);
             }
            });
          //.subscribe(accounts => this.accountTableService.reloadGrid$.next(accounts));
        }
        else if(data == -1)
        {
          console.log("UnExpected error");
          this.accountService.showError('Unexpected Error Occurred');
        }
    
      });
    }

  onSelectCommunication(channelValue : String)
  {
    console.log("on select called");
    this.channel =channelValue;
    console.log("this.channel");
    if(this.channel == "Email")
    {
      this.isEmail=true;
      this.isFax =false;
    }
    else if(this.channel == "Fax")
    {
      this.isFax = true;
      this.isEmail=false;
      this.emailForm.emails =[];
    }
    else if(this.channel =="Both")
    {
      this.isEmail =true;
      this.isFax =true;
    }
    else
    {
      this.isEmail =false;
      this.isFax =false;
      this.emailForm.emails =[];
    }

  }

  onRowSelected(params)
  {
    var selectedRows = this.gridApi.getSelectedRows();
    var selectedRecipientId = "";
    var selectedFaxNumber = "";
    var selectedEmailAddress = "";
    var selectedSecondFaxNumber ="";
    var  selectedChannel ="";
    var  selectedComments ="";
    selectedRows.forEach(function(selectedRow, index) {
      if (index !== 0) {
        selectedRecipientId += ", ";
        selectedFaxNumber += ", ";
        selectedEmailAddress += ", ";
        selectedSecondFaxNumber += ", ";
        selectedChannel += ", ";
        selectedComments += ", ";
      }
      selectedRecipientId += selectedRow.recipientId;
      selectedFaxNumber +=selectedRow.faxNumber;
      selectedSecondFaxNumber +=selectedRow.secondFaxNumber;
      selectedEmailAddress +=selectedRow.emailAddress;
      selectedChannel +=selectedRow.communicationMode;
      selectedComments +=selectedRow.comments;
    });
    
    this.recipientId = selectedRecipientId;
    this.selectedRecipientId =selectedRecipientId;
    
 
    if(selectedFaxNumber !== "undefined")
    {  
      this.isFax =true;
      this.faxNumber =selectedFaxNumber;
      this.selectedFaxNumber =selectedFaxNumber;
      if(selectedSecondFaxNumber !== "undefined")
      {
        this.secondfaxNumber =selectedSecondFaxNumber;
        this.selectedSecondFaxNumber =selectedSecondFaxNumber;
      }
      else{
        this.secondfaxNumber ="";
        this.selectedSecondFaxNumber ="";
      }
    }
    else{
      this.faxNumber ="";
      this.selectedFaxNumber="";
      this.isFax =false;
    }

    if(selectedChannel!== "undefined")
    {
      this.channel =selectedChannel;
      this.selectedChannel =selectedChannel;
    }

    if(selectedEmailAddress !== "undefined")
    {
      this.isEmail = true;
      //this.email =selectedEmailAddress;
  	  this.selectedEmailAddress =selectedEmailAddress;
       var emailAdd =selectedEmailAddress;
      this.emailForm.emails =[];
  
      if(emailAdd.includes(';'))
      {
        let str :string[]=emailAdd.split(';');
        this.email = str[0];
        for(let i=1;i<str.length;i++)
        {
         // console.log("array in loop is",str[i]);
          this.emailForm.emails.push({
           id: i, // <--- uniqueness hook.
           email : str[i]
              });
        }
         
      }
      else{
        this.email =emailAdd;
      } 
    }
    else{
      this.email ="";
      this.selectedEmailAddress="";
      //this.emailForm.emails.splice(0,this.emailForm.emails.length)
      this.isEmail = false;
    }

    if(selectedComments !== "undefined")
    {
      this.comments =selectedComments;
      this.selectedComments =selectedComments;
    }
    else{
      this.comments ="";
      this.selectedComments ="";
    }
  }

  updateRecipientDetails()
  {
    console.log(this.email);
    this.emailForm.emails.forEach(function (value) {
      console.log(value.email);})
      var emailAdresses = this.email;
      if(this.emailForm.emails.length > 0)
      {
        this.emailForm.emails.forEach(function (emailAdd) {
          emailAdresses =emailAdresses +";"+emailAdd.email;
          })
      }
    if(this.selectedRecipientId == this.recipientId && this.selectedChannel == this.channel
       && this.selectedEmailAddress == emailAdresses
     && this.selectedFaxNumber == this.faxNumber && this.selectedSecondFaxNumber ==this.secondfaxNumber 
     && this.selectedComments == this.comments)
     {
        this.accountService.showWarning("Nothing to Update!!");
     }
     else{
       this.recipientDetailsObj = {recipientId : this.recipientId, 
      communicationMode :this.channel,emailAddress :emailAdresses,faxNumber :this.faxNumber,
      secondFaxNumber:this.secondfaxNumber,status :this.activated,createdBy :sessionStorage.getItem("userDetails"),
      updatedBy :sessionStorage.getItem("userDetails"),comments:this.comments};
      console.log(this.recipientDetailsObj);
     
        this.accountService.
        createNewCMAccountRegion(
          "/MOLandingZoneGateway/api/emailFax/updateRecipient", this.recipientDetailsObj)
          .subscribe(data =>
          {
            console.log(data);
            if (data == 1) {
              console.log("Updated Successfully");
             this.accountService.showSuccess('Recipient Details successfully updated');
             this.accountService.getMonitoringDetails
             ("/MOLandingZoneGateway/api/emailFax/details")
               .subscribe(recipientDetails => 
                { this.recipientDetails = recipientDetails;
                 console.log(this.recipientDetails)
                 if(this.recipientDetails ==null)
                 {
                   this.accountService.showError('Unable to retrieve the account details');
                 }
                });
                this.selectedRecipientId =this.recipientId;
                this.selectedChannel = this.channel;
                this.selectedEmailAddress = emailAdresses;
                this.selectedFaxNumber = this.faxNumber ;
                this.selectedSecondFaxNumber =this.secondfaxNumber; 
                this.selectedComments = this.comments;
                
          }
          if (data == 2) {
            console.log("Created Successfully");
            this.accountService.showSuccess('Recipient created Successfully');
            this.accountService.getMonitoringDetails
            ("/MOLandingZoneGateway/api/emailFax/details")
              .subscribe(recipientDetails => 
               { this.recipientDetails = recipientDetails;
                console.log(this.recipientDetails)
                if(this.recipientDetails ==null)
                {
                  this.accountService.showError('Unable to retrieve the account details');
                }
               });
               this.selectedRecipientId =this.recipientId;
               this.selectedChannel = this.channel;
               this.selectedEmailAddress = emailAdresses;
               this.selectedFaxNumber = this.faxNumber ;
               this.selectedSecondFaxNumber =this.secondfaxNumber; 
               this.selectedComments = this.comments;
             
        }
          else if(data == -1)
          {
            this.accountService.showError('Unexpected Error occurred!!');
          }
          });
        }
   
  }

  onGridReady(params) {
    this.gridApi = params.api;
    this.gridColumnApi = params.columnApi;
    this.gridApi.sizeColumnsToFit()
  }

  onFilterTextBoxChanged() 
  {
   var accountNumberComponent = this.gridApi.getFilterInstance("recipientId");
   accountNumberComponent.setModel({
     type: "startsWith",
     filter: this.searchText
   });
   this.gridApi.onFilterChanged();
  }

  ngOnDestroy(): void
  {
    this.reloadEmailFaxGrid.unsubscribe();
  }

}

