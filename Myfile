  <button type="button" class="btn btn-sm btn-primary" *ngIf="parameters && activateButton" 
   style="width:130px;padding:1px 11px;margin-bottom:6px;" (click)="ViewNackReason()" >
   View Nack Reason</button>
  </div>
  
