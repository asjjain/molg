active-recipient.component.html
********************************************************************************
<div>
  <button type="button" class="btn btn-sm btn-success"*ngIf="isActivate"
   style="width:79px;padding:1px 11px;margin-bottom:6px;" (click)="activateRecipient()">
   Active</button>
  <button type="button" class="btn btn-sm btn-danger" *ngIf="isDeactivate" 
  style="padding:1px 11px;margin-bottom:6px;" (click)="deactivateRecipient()"
  >
  Deactivate</button>
  
  </div>

********************************************************************************
active-recipient.component.ts
********************************************************************************

import { Component, ElementRef, ViewChild } from '@angular/core';
import { INoRowsOverlayAngularComp } from 'ag-grid-angular';
import { AccountTableService } from '../shared/account-table.service';

@Component({
  selector: 'app-active-recipient',
  templateUrl: './active-recipient.component.html',
  styleUrls: ['./active-recipient.component.css']
})
export class ActiveRecipientComponent implements INoRowsOverlayAngularComp {

  @ViewChild('comments') searchElement: ElementRef;
  private parameters:any;
  private userName: String = sessionStorage.getItem("userDetails");
  isActivate:boolean =false;
  isDeactivate:boolean =false;
  private flag:boolean;
  private selectedRowObject:any;

  constructor(private accountService: AccountTableService,
    private el: ElementRef) { }

  agInit(params){
    this.parameters =params.data;

      if(this.parameters.updatedBy != this.userName)
      {
        this.flag =true;
      }
      
    if(this.flag)
     { 
       if(this.parameters.status=="ACTIVE")
       {
          this.isDeactivate =true;
       }
       else  if(this.parameters.status=="DEACTIVE")
       {
          this.isActivate =true;
       }
     }
  }



  activateRecipient()
  {
    console.log("activate recipient called");   
  
    this.selectedRowObject ={
      recipientId:this.parameters.recipientId,
      communicationMode:this.parameters.communicationMode,
      faxNumber:this.parameters.faxNumber,
      secondFaxNumber :this.parameters.secondFaxNumber,
      emailAddress:this.parameters.emailAddress,
      status:this.parameters.status,
      createdBy:this.parameters.createdBy,
      creationDate:this.parameters.creationDate,
      updatedBy:this.parameters.updatedBy,
      updatedDate:this.parameters.updatedDate,
      validatedBy:sessionStorage.getItem("userDetails"),
      comments:this.parameters.comments,
      type:'activate'
    }
    this.accountService.loadEmailFaxModal.next(this.selectedRowObject);
    console.log("activate Object:", this.selectedRowObject);
  }

  deactivateRecipient()
  {
    console.log("deactivate recipient called");
      this.selectedRowObject ={
        recipientId:this.parameters.recipientId,
        communicationMode:this.parameters.communicationMode,
        faxNumber:this.parameters.faxNumber,
        secondFaxNumber :this.parameters.secondFaxNumber,
        emailAddress:this.parameters.emailAddress,
        status:this.parameters.status,
        createdBy:this.parameters.createdBy,
        creationDate:this.parameters.creationDate,
        updatedBy:this.parameters.updatedBy,
        updatedDate:this.parameters.updatedDate,
        validatedBy:sessionStorage.getItem("userDetails"),
        comments:this.parameters.comments,
        type:'deactivate'
      }
      this.accountService.loadEmailFaxModal.next(this.selectedRowObject);
      console.log("deactivate Object:", this.selectedRowObject);
  }

}

